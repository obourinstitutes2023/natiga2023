function formatDate(ss) {

    if (ss == "") return ("");

    var x = 8;
    var y, m, d;
    var ds = "";

    if (ss.charAt(0) == "y") {
        x = 1;
        ds += forums_date_yesterday;
    }
    else if (ss.length == 6) {
        x = 0;
        ds += forums_date_today;
    }
    else if (ss.length == 8) {
        x = 2;
        if (thisMonth < 10) m = "0" + thisMonth;
        else m = thisMonth;
        d = ss.substr(0, 2);
        ds += m + "/" + d;
    }
    else if (ss.length == 10) {
        x = 4;
        m = ss.substr(0, 2);
        d = ss.substr(2, 2);
        ds += m + "/" + d;
    }
    else if (ss.length == 14) {
        x = 8;
        y = ss.substr(0, 4);
        m = ss.substr(4, 2);
        d = ss.substr(6, 2);
        ds += y + "/" + m + "/" + d;
    }

    var h = ss.substr(x, 2);
    var n = ss.substr(x + 2, 2);
    var s = ss.substr(x + 4, 2);

    if (memberLevel > 1)
        ds = h + ":" + n + ":" + s + " - " + ds;
    else
        ds = h + ":" + n + " - " + ds;

    return (ds);
}

function formatDateFull(ss) {
    if (ss == "") return ("");

    var x = 8;
    var y, m, d;
    var ds = "";
    y = thisYear;
    m = thisMonth;
    d = thisDay;

    if (ss.charAt(0) == "y") {
        x = 1;
        var myDate = new Date();
        myDate.setFullYear(thisYear, thisMonth - 1, thisDay);
        myDate.setDate(myDate.getDate() - 1);
        y = myDate.getFullYear();
        m = myDate.getMonth() + 1;
        d = myDate.getDate();
    }
    else if (ss.length == 6) {
        x = 0;
    }
    else if (ss.length == 8) {
        x = 2;
        if (thisMonth < 10) m = "0" + thisMonth;
        else m = thisMonth;
        d = ss.substr(0, 2);
    }
    else if (ss.length == 10) {
        x = 4;
        m = ss.substr(0, 2);
        d = ss.substr(2, 2);
    }
    else if (ss.length == 14) {
        x = 8;
        y = ss.substr(0, 4);
        m = ss.substr(4, 2);
        d = ss.substr(6, 2);
    }
    ds += y + "/" + m + "/" + d;

    var h = ss.substr(x, 2);
    var n = ss.substr(x + 2, 2);
    var s = ss.substr(x + 4, 2);

    if (memberLevel > 1)
        ds = h + ":" + n + ":" + s + " - " + ds;
    else
        ds = h + ":" + n + " - " + ds;

    return (ds);
}


